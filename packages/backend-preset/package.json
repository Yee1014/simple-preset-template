{
  "name": "backend-preset",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:beta": "vue-cli-service build --mode beta",
    "build:dev": "vue-cli-service build --mode dev",
    "build:uat": "vue-cli-service build --mode uat",
    "build:prod": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.5.0",
    "@vue/composition-api": "^0.5.0",
    "axios": "^0.19.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.14",
    "decimal.js": "^10.2.0",
    "echarts": "^4.7.0",
    "element-ui": "^2.4.5",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.11",
    "prismjs": "^1.22.0",
    "screenfull": "^5.0.2",
    "storage-manager-js": "^4.0.4",
    "tailwindcss": "^1.6.2",
    "vue": "^2.6.11",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.24.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-unused-vars": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-debugger": "off",
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ]
    },
    "globals": {
      "_hmt": true,
      "uni": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": "./tailwind.config.js"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
